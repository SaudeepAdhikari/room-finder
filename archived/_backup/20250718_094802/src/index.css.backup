/* Interactive hover backgrounds */
.interactive-bg {
  position: relative;
  overflow: hidden;
  transition: all 0.3s ease;
}

.interactive-bg::before {
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  width: 0;
  height: 0;
  background: radial-gradient(circle, rgba(139, 92, 246, 0.1) 0%, transparent 70%);
  transition: all 0.6s ease;
  transform: translate(-50%, -50%);
  border-radius: 50%;
}

.interactive-bg:hover::before {
  width: 200%;
  height: 200%;
}

/* Elegant dark mode backgrounds */
@media (prefers-color-scheme: dark) {
  .hero-background {
    background: linear-gradient(135deg, 
      rgba(79, 70, 229, 0.9) 0%, 
      rgba(59, 130, 246, 0.8) 25%,
      rgba(6, 182, 212, 0.7) 50%,
      rgba(16, 185, 129, 0.8) 75%,
      rgba(139, 92, 246, 0.9) 100%);
  }

  .search-section-bg {
    background: linear-gradient(165deg,
      rgba(15, 23, 42, 0.98) 0%,
      rgba(30, 41, 59, 0.95) 50%,
      rgba(51, 65, 85, 0.92) 100%);
  }

  .premium-section-bg {
    background: 
      radial-gradient(circle at 25% 25%, rgba(139, 92, 246, 0.15) 0%, transparent 50%),
      radial-gradient(circle at 75% 75%, rgba(59, 130, 246, 0.12) 0%, transparent 50%),
      linear-gradient(135deg, rgba(15, 23, 42, 0.95) 0%, rgba(30, 41, 59, 0.9) 100%);
  }
}

/* Modern glassmorphism effects */
.glass-morphism {
  background: rgba(255, 255, 255, 0.1);
  -webkit-backdrop-filter: blur(20px);
  backdrop-filter: blur(20px);
  border: 1px solid rgba(255, 255, 255, 0.2);
  box-shadow: 
    0 8px 32px rgba(31, 38, 135, 0.37),
    inset 0 1px 0 rgba(255, 255, 255, 0.3),
    inset 0 -1px 0 rgba(255, 255, 255, 0.1);
}

/* Airbnb-style image overlays */
.image-overlay {
  position: relative;
  overflow: hidden;
}

.image-overlay::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: linear-gradient(
    to bottom,
    rgba(0, 0, 0, 0) 0%,
    rgba(0, 0, 0, 0.1) 50%,
    rgba(0, 0, 0, 0.3) 100%
  );
  opacity: 0;
  transition: opacity 0.3s ease;
}

.image-overlay:hover::after {
  opacity: 1;
}

/* Responsive optimizations */
@media (max-width: 768px) {
  .hero-background::before,
  .hero-background::after {
    animation-duration: 6s;
  }
  
  .floating-element {
    width: 15px;
    height: 15px;
  }
}