/* Critical scroll performance fixes */
html {
  scroll-behavior: smooth;
  overflow-x: hidden;
}

body {
  scroll-behavior: smooth;
  overflow-x: hidden;
  -ms-overflow-style: -ms-autohiding-scrollbar;
}

/* Disable problematic animations during scroll */
.disable-hover-on-scroll * {
  pointer-events: none !important;
}

/* Optimize all animations for performance */
* {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  -webkit-transform-style: preserve-3d;
  transform-style: preserve-3d;
}

/* GPU acceleration for smoother performance */
.gpu-layer {
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
  will-change: transform;
}

/* Disable expensive effects during scroll */
@media (hover: hover) and (pointer: fine) {
  .optimized-hover:hover {
    transition: transform 0.2s ease-out;
  }
}

/* Reduce animation complexity */
.simple-animation {
  transition: all 0.2s ease-out;
}

/* Optimize backdrop filters */
.backdrop-optimized {
  -webkit-backdrop-filter: blur(8px);
  backdrop-filter: blur(8px);
}

/* Prevent layout shifts during animations */
.no-layout-shift {
  contain: layout style paint;
}

/* Smooth page transitions */
.page-transition {
  will-change: transform, opacity;
  backface-visibility: hidden;
  perspective: 1000px;
}

/* Optimize multi-step form animations */
.multi-step-container {
  contain: layout;
  transform: translateZ(0);
}

.step-indicator {
  will-change: transform, background-color;
  contain: layout style;
}

/* Reduce motion for accessibility */
@media (prefers-reduced-motion: reduce) {
  *, *::before, *::after {
    animation-duration: 0.01ms !important;
    animation-iteration-count: 1 !important;
    transition-duration: 0.01ms !important;
    scroll-behavior: auto !important;
  }
}

/* Custom animation utilities */
@layer utilities {
  .animate-fade-in-up {
    animation: fade-in-up 0.6s ease-out;
  }
  
  .animate-fade-in-down {
    animation: fade-in-down 0.6s ease-out;
  }
  
  .animate-slide-in-left {
    animation: slide-in-left 0.5s ease-out;
  }
  
  .animate-slide-in-right {
    animation: slide-in-right 0.5s ease-out;
  }
  
  .animate-scale-in {
    animation: scale-in 0.4s ease-out;
  }
  
  .animate-bounce-in {
    animation: bounce-in 0.6s ease-out;
  }
  
  .animate-glow {
    animation: glow 2s ease-in-out infinite alternate;
  }
  
  .animate-float {
    animation: float 3s ease-in-out infinite;
  }
  
  .animate-gradient {
    animation: gradient-shift 3s ease infinite;
    background-size: 200% 200%;
  }
  
  .gentle-animation {
    animation-fill-mode: both;
    animation-timing-function: ease-out;
  }
}

/* Keyframe definitions */
@keyframes fade-in-up {
  from {
    opacity: 0;
    transform: translateY(30px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes fade-in-down {
  from {
    opacity: 0;
    transform: translateY(-30px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes slide-in-left {
  from {
    opacity: 0;
    transform: translateX(-50px);
  }
  to {
    opacity: 1;
    transform: translateX(0);
  }
}

@keyframes slide-in-right {
  from {
    opacity: 0;
    transform: translateX(50px);
  }
  to {
    opacity: 1;
    transform: translateX(0);
  }
}

@keyframes scale-in {
  from {
    opacity: 0;
    transform: scale(0.9);
  }
  to {
    opacity: 1;
    transform: scale(1);
  }
}

@keyframes bounce-in {
  0% {
    opacity: 0;
    transform: scale(0.3);
  }
  50% {
    opacity: 1;
    transform: scale(1.05);
  }
  70% {
    transform: scale(0.9);
  }
  100% {
    opacity: 1;
    transform: scale(1);
  }
}

@keyframes glow {
  from {
    box-shadow: 0 0 5px rgba(124, 58, 237, 0.5);
  }
  to {
    box-shadow: 0 0 20px rgba(124, 58, 237, 0.8);
  }
}

@keyframes float {
  0%, 100% {
    transform: translateY(0px);
  }
  50% {
    transform: translateY(-10px);
  }
}

@keyframes gradient-shift {
  0%, 100% {
    background-position: 0% 50%;
  }
  50% {
    background-position: 100% 50%;
  }
}

/* Enhanced hover effects */
.hover-lift {
  transition: all 0.3s ease;
}

.hover-lift:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 25px rgba(0, 0, 0, 0.15);
}

.hover-glow {
  transition: all 0.3s ease;
}

.hover-glow:hover {
  box-shadow: 0 0 20px rgba(124, 58, 237, 0.4);
}

/* Airbnb-inspired heading styles */
.hero-heading {
  background: linear-gradient(135deg, 
    rgba(255, 255, 255, 0.98) 0%,
    rgba(248, 250, 252, 0.95) 100%);
  -webkit-backdrop-filter: blur(25px);
  backdrop-filter: blur(25px);
  border: 1px solid rgba(255, 255, 255, 0.3);
  border-radius: 24px;
  padding: 3rem 2rem;
  position: relative;
  overflow: hidden;
  box-shadow: 
    0 20px 60px rgba(0, 0, 0, 0.1),
    0 8px 32px rgba(139, 92, 246, 0.15);
}

.hero-heading::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 3px;
  background: linear-gradient(90deg, #8b5cf6, #3b82f6, #06b6d4, #10b981);
  background-size: 300% 100%;
  animation: rainbow-flow 4s ease infinite;
}

@keyframes rainbow-flow {
  0%, 100% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
}

.section-header {
  position: relative;
  padding: 2rem 0;
  background: radial-gradient(ellipse at top, rgba(139, 92, 246, 0.05) 0%, transparent 70%);
}

.content-block {
  background: rgba(255, 255, 255, 0.8);
  -webkit-backdrop-filter: blur(12px);
  backdrop-filter: blur(12px);
  border: 1px solid rgba(139, 92, 246, 0.1);
  border-radius: 20px;
  padding: 2.5rem;
  margin: 2rem 0;
  position: relative;
  transition: all 0.4s ease;
}

.content-block:hover {
  transform: translateY(-8px);
  box-shadow: 0 25px 80px rgba(139, 92, 246, 0.15);
}

/* Gradient backgrounds */
.bg-gradient-purple-blue {
  background: linear-gradient(135deg, #7c3aed 0%, #3b82f6 100%);
}

.bg-gradient-blue-cyan {
  background: linear-gradient(135deg, #3b82f6 0%, #06b6d4 100%);
}

.bg-gradient-purple-pink {
  background: linear-gradient(135deg, #7c3aed 0%, #ec4899 100%);
}

/* Glass morphism */
.glass-effect {
  background: rgba(255, 255, 255, 0.25);
  -webkit-backdrop-filter: blur(10px);
  backdrop-filter: blur(10px);
  border: 1px solid rgba(255, 255, 255, 0.18);
}

.glass-effect-dark {
  background: rgba(30, 41, 59, 0.25);
  -webkit-backdrop-filter: blur(10px);
  backdrop-filter: blur(10px);
  border: 1px solid rgba(255, 255, 255, 0.1);
}

/* Enhanced animations for multi-step form */
@keyframes shine {
  0% {
    left: -100%;
  }
  100% {
    left: 100%;
  }
}

@keyframes pulse-glow {
  0%, 100% {
    box-shadow: 0 4px 16px rgba(139, 92, 246, 0.2);
  }
  50% {
    box-shadow: 0 6px 24px rgba(139, 92, 246, 0.3);
  }
}

@keyframes float {
  0%, 100% {
    transform: translateY(0px);
  }
  50% {
    transform: translateY(-10px);
  }
}

/* Glass morphism enhancement */
.glass-card {
  background: rgba(255, 255, 255, 0.85) !important;
  -webkit-backdrop-filter: blur(12px) !important;
  backdrop-filter: blur(12px) !important;
  border: 1px solid rgba(139, 92, 246, 0.15) !important;
}

body {
  margin: 0;
  font-family: 'Poppins', 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen',
    'Ubuntu', 'Cantarell', 'Fira Sans', 'Droid Sans', 'Helvetica Neue',
    sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  background: linear-gradient(135deg, #f8fafc 0%, #f1f5f9 50%, #e2e8f0 100%);
  background-attachment: fixed;
  min-height: 100vh;
}

code {
  font-family: source-code-pro, Menlo, Monaco, Consolas, 'Courier New',
    monospace;
}

/* Custom scrollbar for smooth experience */
::-webkit-scrollbar {
  width: 8px;
  height: 8px;
}

::-webkit-scrollbar-track {
  background: rgba(241, 245, 249, 0.5);
  border-radius: 4px;
}

::-webkit-scrollbar-thumb {
  background: linear-gradient(135deg, rgba(139, 92, 246, 0.6) 0%, rgba(59, 130, 246, 0.6) 100%);
  border-radius: 4px;
  transition: all 0.3s ease;
}

::-webkit-scrollbar-thumb:hover {
  background: linear-gradient(135deg, rgba(139, 92, 246, 0.8) 0%, rgba(59, 130, 246, 0.8) 100%);
}

/* Firefox scrollbar */
html {
  scrollbar-width: thin;
  scrollbar-color: rgba(139, 92, 246, 0.6) rgba(241, 245, 249, 0.5);
}

/* Optimize animations for better performance */
@media (prefers-reduced-motion: no-preference) {
  .floating-element {
    animation: gentle-float 8s ease-in-out infinite;
  }
  
  .rotating-element {
    animation: gentle-rotate 30s linear infinite;
  }
}

@keyframes gentle-float {
  0%, 100% { transform: translateY(0px) translateX(0px); }
  25% { transform: translateY(-8px) translateX(4px); }
  50% { transform: translateY(4px) translateX(-2px); }
  75% { transform: translateY(-4px) translateX(6px); }
}

@keyframes gentle-rotate {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}

/* Improve text rendering */
* {
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

/* Airbnb-inspired elegant backgrounds and animations */

/* Hero section gradient background with overlay patterns */
.hero-background {
  background: linear-gradient(135deg, 
    rgba(139, 92, 246, 0.9) 0%, 
    rgba(59, 130, 246, 0.8) 25%,
    rgba(6, 182, 212, 0.7) 50%,
    rgba(16, 185, 129, 0.8) 75%,
    rgba(139, 92, 246, 0.9) 100%);
  background-size: 400% 400%;
  animation: gradient-flow 8s ease-in-out infinite;
  position: relative;
  overflow: hidden;
}

/* Floating geometric shapes background */
.hero-background::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: 
    radial-gradient(circle at 20% 30%, rgba(255, 255, 255, 0.1) 0%, transparent 50%),
    radial-gradient(circle at 80% 70%, rgba(255, 255, 255, 0.08) 0%, transparent 50%),
    radial-gradient(circle at 40% 80%, rgba(255, 255, 255, 0.06) 0%, transparent 50%);
  animation: float-shapes 12s ease-in-out infinite;
}

/* Moving particles effect */
.hero-background::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: 
    url("data:image/svg+xml,%3Csvg width='60' height='60' viewBox='0 0 60 60' xmlns='http://www.w3.org/2000/svg'%3E%3Cg fill='none' fill-rule='evenodd'%3E%3Cg fill='%23ffffff' fill-opacity='0.05'%3E%3Ccircle cx='30' cy='30' r='2'/%3E%3C/g%3E%3C/g%3E%3C/svg%3E");
  animation: move-particles 20s linear infinite;
}

@keyframes gradient-flow {
  0%, 100% { background-position: 0% 50%; }
  25% { background-position: 100% 50%; }
  50% { background-position: 100% 100%; }
  75% { background-position: 0% 100%; }
}

@keyframes float-shapes {
  0%, 100% { transform: translateY(0px) rotate(0deg); }
  25% { transform: translateY(-20px) rotate(5deg); }
  50% { transform: translateY(-10px) rotate(-3deg); }
  75% { transform: translateY(-30px) rotate(7deg); }
}

@keyframes move-particles {
  0% { transform: translateX(0) translateY(0); }
  100% { transform: translateX(-60px) translateY(-60px); }
}

/* Airbnb-style card backgrounds */
.property-card-bg {
  background: linear-gradient(145deg, 
    rgba(255, 255, 255, 0.95) 0%,
    rgba(248, 250, 252, 0.9) 100%);
  -webkit-backdrop-filter: blur(20px);
  backdrop-filter: blur(20px);
  border: 1px solid rgba(255, 255, 255, 0.2);
  position: relative;
  overflow: hidden;
}

.property-card-bg::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, 
    transparent, 
    rgba(139, 92, 246, 0.1), 
    transparent);
  transition: left 0.8s ease;
}

.property-card-bg:hover::before {
  left: 100%;
}

/* Dark mode property cards */
@media (prefers-color-scheme: dark) {
  .property-card-bg {
    background: linear-gradient(145deg, 
      rgba(30, 41, 59, 0.95) 0%,
      rgba(15, 23, 42, 0.9) 100%);
    border: 1px solid rgba(139, 92, 246, 0.2);
  }
}

/* Search section elegant background */
.search-section-bg {
  background: linear-gradient(165deg,
    rgba(255, 255, 255, 0.98) 0%,
    rgba(248, 250, 252, 0.95) 50%,
    rgba(241, 245, 249, 0.92) 100%);
  -webkit-backdrop-filter: blur(15px);
  backdrop-filter: blur(15px);
  position: relative;
}

/* Animated wave background */
.wave-background {
  background: linear-gradient(-45deg, #667eea 0%, #764ba2 100%);
  background-size: 400% 400%;
  animation: gradient-wave 6s ease infinite;
  position: relative;
}

.wave-background::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
